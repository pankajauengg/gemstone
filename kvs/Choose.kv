<Choose>:
    
    
    BoxLayout:
        orientation:'horizontal'
    
        BoxLayout:
            orientation: 'vertical'
            FileChooserIconView:
                id: filechooser
                on_selection: root.Choosen(filechooser.selection)
                size_hint_y: .9
                
            Button:
                text:'Back'
                on_release: root.manager.current  = "add" 
                size_hint_y: .1
        
        BoxLayout:
            orientation: 'vertical'
            BoxLayout:
                orientation: 'vertical'
                size_hint_y: .9
                Image:
                    id: image
                    size_hint: None, None
                    size: 400, 400
                    #pos_hint: None, None
                    source: "selection/+.png"
            Button:
                text:'Select'
                on_release: root.SelChoosen()
                size_hint_y: .1